		"SUBCMD2": 28,
		"MSGID": 7,
		"VOICEID": 36260,
		"NameJPN": "紬",
		"JPN": "「わたしは……シズクと一緒にいて楽しいからです」",
		"NameENG": "Tsumugi",
		"ENG": "\"Because it's fun to be with you.\"",
		"Args": "0b"
	},
