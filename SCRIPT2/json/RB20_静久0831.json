		"SUBCMD2": 16,
		"MSGID": 1,
		"VOICEID": 36254,
		"NameJPN": "紬",
		"JPN": "「誰よりも優しいシズク……」",
		"NameENG": "Tsumugi",
		"ENG": "\"Shizuku―the kindest of them all...\"",
		"Args": "0b"
	},
	{
